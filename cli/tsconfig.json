{
  "extends": "../tsconfig.json",
  "include": ["src"],
  "compilerOptions": {
    "module": "ES2015",
    "moduleResolution": "node", // Setting node16 causes a compile error on import from antlr4

    // Avoid compile errors of SQLiteParser.ts
    "exactOptionalPropertyTypes": false,
    "noImplicitOverride": false
  }
}
